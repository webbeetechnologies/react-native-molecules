{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "strict": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "module": "preserve",
        "types": ["react", "@docusaurus/module-type-aliases"],
        "moduleResolution": "bundler",
        "resolveJsonModule": true
    },
    "include": ["./src/**/*", "./docs/**/*", "./docusaurus.config.js"]
}
